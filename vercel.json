{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/pallet-calculator", "destination": "/", "permanent": true },
    { "source": "/pallet-calculator/", "destination": "/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|svg|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, s-maxage=31536000, stale-while-revalidate=60" }
      ]
    }
  ]
}
